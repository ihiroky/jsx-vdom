{
  "compilerOptions": {
    "target": "es3",
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "MyJSXVDOM",
    "outDir": "./dist/js",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["*.ts", "**/*.ts", "**/*.tsx"]
}
